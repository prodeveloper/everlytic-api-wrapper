{
    "name": "chencha/everlytic",
    "description": "Provides a simple and elegant wrapper for the Everlytic Bulk Mail API to enable use within laravel framework",
    "keywords":["everlytic","email","bulk"],,
    "authors": [
        {
            "name": "Chencha Jacob",
            "email": "jacob@chenchatech.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*"
    },
    "autoload": {
        "classmap": [
            "src/lib",
            "src/Facades",
            "src/Everlytic.php"
        ],
        "psr-0": {
            "Chencha\\Everlytic\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
